					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="CollectingAcquisitionSource"
				minOccurs="0" type="CollectingAcquisitionSource" sawsdl:modelReference="http://rs.tdwg.org/abcd/terms/collectingAcquisitionSource">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="OtherIdentification" minOccurs="0" type="String" sawsdl:modelReference="http://rs.tdwg.org/abcd/terms/otherIdentification">
