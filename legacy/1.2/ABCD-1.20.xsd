														<xs:appinfo>
															<sea:FullName>Relation to nearest named place</sea:FullName>
															<sea:Audience>BioCASE</sea:Audience>
															<sea:Audience>CODATA TDWG</sea:Audience>
															<sea:Reviewer/>
															<sea:ExistingStandards/>
															<sea:Content/>
