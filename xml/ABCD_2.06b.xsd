					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="DecodedDonorInstitute" minOccurs="0" type="String" sawsdl:modelReference="http://rs.tdwg.org/abcd/terms/decodedGatheringInstitute">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="DecodedLocationSafetyDuplicates" minOccurs="0" type="String" sawsdl:modelReference="http://rs.tdwg.org/abcd/terms/decodedLocationSafetyDuplicates">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="GatheringInstitutionCode" minOccurs="0" type="FAOInstituteCode" sawsdl:modelReference="http://rs.tdwg.org/abcd/terms/gatheringInstitutionCode">
				<xs:annotation>
					<xs:documentation>FAO Institute Code of the institute that has collected the material.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AccessionNames" minOccurs="0" >
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AccessionName"
							minOccurs="0" maxOccurs="unbounded" type="String" sawsdl:modelReference="http://rs.tdwg.org/abcd/terms/accessionName">
							<xs:annotation>
								<xs:documentation>A name given to the accession that cannot be covered by the name structure given for botanical names under identification (i.e. not a formal cultivar or trade designation name).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AccessionNameText" minOccurs="0" type="String" sawsdl:modelReference="http://rs.tdwg.org/abcd/terms/accessionNameText">
